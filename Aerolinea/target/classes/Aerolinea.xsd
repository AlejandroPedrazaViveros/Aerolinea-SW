<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/Aerolinea" 
	xmlns:tns="http://www.example.org/Aerolinea" 
	elementFormDefault="qualified">

	<element name="ReservarVueloRequest">
          <complexType>
            <sequence>
              <element type="string" name="pasajero"/>
              <element type="string" name="salida"/>
              <element type="string" name="destino"/>
              <element type="string" name="fecha"/>
              <element type="string" name="hora"/>
              <element type="string" name="asiento"/>
              <element type="int" name="boleto"/>
            </sequence>
          </complexType>
        </element>
        <element name="ReservarVueloResponse">
          <complexType>
            <sequence>
              <element type="string" name="confirmar"/>
            </sequence>
          </complexType>
        </element>
        <element name="ModificarDatosRequest">
          <complexType>
            <sequence>
              <element type="string" name="pasajero"/>
              <element type="string" name="salida"/>
              <element type="string" name="destino"/>
              <element type="string" name="fecha"/>
              <element type="string" name="hora"/>
              <element type="string" name="asiento"/>
              <element type="int" name="boleto"/>
            </sequence>
          </complexType>
        </element>
        <element name="ModificarDatosResponse">
          <complexType>
            <sequence>
              <element type="string" name="datosmodificados"/>
            </sequence>
          </complexType>
        </element>
        <element name="CancelarReservacionRequest">
          <complexType>
            <sequence>
              <element type="int" name="boleto"/>
            </sequence>
          </complexType>
        </element>
        <element name="CancelarReservacionResponse">
          <complexType>
            <sequence>
              <element type="string" name="cancelado"/>
            </sequence>
          </complexType>
        </element>
	
</schema>